{
  "exec-opts": ["native.cgroupdriver=systemd"],
  "data-root": "/home/docker",
  "insecure-registries":["k8s.gcr.io","gcr.io","quay.io","harbor.io","{{ urls.registry.url }}:{{ urls.registry.port }}"],
  "registry-mirrors": [],
  "log-driver": "json-file",
  "log-opts": {
    "max-size": "100m"
  },
  "storage-driver": "overlay2",
  "storage-opts": [
    "overlay2.override_kernel_check=true"
  ]
}